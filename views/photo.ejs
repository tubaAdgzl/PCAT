<%- include('partials/_header') -%>

            <%- include('partials/_navigation') -%>

	
		<!-- Page content -->
		<div class="container-fluid">
			<div class="mx-auto tm-content-container">
				<main>
					<div class="row mb-5 pb-4">
						<div class="col-12">
							<img src="<%= photo.image %>" alt="Image" class="img-fluid tm-catalog-item-img">
						</div>
					</div>
					<div class="row mb-5 pb-5">
						<div class="col-xl-8 col-lg-7">
							<!-- Video description -->
							<div class="tm-video-description-box">
								<h2 class="mb-5 tm-video-title"> <%= photo.title %> </h2>
								<p class="mb-4"><%= photo.description %></p>	
							</div>							
						</div>
						<div class="col-xl-4 col-lg-5">
							<!-- Share box -->
							<div class="tm-bg-gray tm-share-box">
								<div class="text-center">
									<a href="/edit/<%= photo._id %>" class="btn btn-primary p-0 mb-4 tm-btn-animate tm-btn-download tm-icon-download"><span>Update Details</span></a>	
								</div>	
								<div class="text-center">
									<a href="/photos/<%= photo._id %>?_method=DELETE"
										onclick="return confirm('Are you sure?')" class="btn btn-danger p-0 tm-btn-animate tm-btn-download tm-icon-download"><span>Delete Photo</span></a>	
								</div>								
							</div>
						</div>
					</div>			
				</main>

            <%- include('partials/_footer') -%>
